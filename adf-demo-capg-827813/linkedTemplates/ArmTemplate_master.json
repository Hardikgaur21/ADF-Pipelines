{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-capg-827813"
		},
		"AdvWorksDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksDb'"
		},
		"BlobStorageProduct_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageProduct'"
		},
		"AdvCustomerDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvCustomerDb'"
		},
		"BlobStorageCustomer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageCustomer'"
		},
		"AdvWorksDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver8.database.windows.net"
		},
		"AdvWorksDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-db-82"
		},
		"AdvWorksDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Hardikgaur"
		},
		"AdvCustomerDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver8.database.windows.net"
		},
		"AdvCustomerDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-db-82"
		},
		"AdvCustomerDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Hardikgaur"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-capg-827813_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdvWorksDb_password": {
						"value": "[parameters('AdvWorksDb_password')]"
					},
					"BlobStorageProduct_connectionString": {
						"value": "[parameters('BlobStorageProduct_connectionString')]"
					},
					"AdvCustomerDb_password": {
						"value": "[parameters('AdvCustomerDb_password')]"
					},
					"BlobStorageCustomer_connectionString": {
						"value": "[parameters('BlobStorageCustomer_connectionString')]"
					},
					"AdvWorksDb_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_server')]"
					},
					"AdvWorksDb_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_database')]"
					},
					"AdvWorksDb_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_userName')]"
					},
					"AdvCustomerDb_properties_typeProperties_server": {
						"value": "[parameters('AdvCustomerDb_properties_typeProperties_server')]"
					},
					"AdvCustomerDb_properties_typeProperties_database": {
						"value": "[parameters('AdvCustomerDb_properties_typeProperties_database')]"
					},
					"AdvCustomerDb_properties_typeProperties_userName": {
						"value": "[parameters('AdvCustomerDb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}